{
  "schemaVersion": 1,
  "id": "nbtvoid",
  "version": "${version}",

  "name": "NBT Void",
  "description": "Saves every item containing NBT to the creative menu tab",
  "authors": [
    "White_145"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/nbt-void",
    "sources": "https://github.com/White-145/nbtvoid",
    "issues": "https://github.com/White-145/nbtvoid/issues"
  },

  "license": "MIT",
  "icon": "assets/nbtvoid/icon.png",

  "accessWidener": "nbtvoid.accesswidener",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.white.nbtvoid.NbtVoid"
    ],
    "modmenu": [
      "me.white.nbtvoid.ModMenuIntegration"
    ]
  },
  "mixins": [
    "nbtvoid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric-api": "*",
    "minecraft": "~1.20.1",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
